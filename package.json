{
  "name": "html-comment-template-engine-example",
  "version": "1.0.0",
  "description": "Example of using HTML commenting template engine with Express.js server",
  "main": "server.js",
  "scripts": {
    "dev:man": "nodemon server.js",
    "build:ts": "rimraf dist/ && tsc -p tsconfig.node.json --incremental",
    "cp-assets": "rimraf public/assets && cp -R src/assets public/ && rimraf public/assets/css ",
    "pack": "./node_modules/.bin/webpack",
    "gen:html": "node res/scripts/genHTML.js",
    "gen:ract": "node res/scripts/genRact.js",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run build:ts && npm run gen:html && npm run gen:ract && webpack --config=res/webpack/webpack.config.js --mode=development && npm run cp-assets",
    "build": "npm run build:ts && npm run gen:html && webpack --config=res/webpack/webpack.config.js  --mode=production && npm run cp-assets",
    "watch": "webpack --watch --config=res/webpack/webpack.config.js  --mode=development"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsdom": "^21.1.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.15.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.15",
    "ejs": "^3.1.9",
    "eslint": "^8.36.0",
    "eslint-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "marked": "^4.3.0",
    "mini-css-extract-plugin": "^2.7.5",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.1.0",
    "prettier": "^2.8.7",
    "style-loader": "^3.3.2",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-webpack-plugin": "^4.1.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
}
