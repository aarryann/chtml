  <script defer type="text/javascript">

    const loadJS = (b, script, type = "text/javascript") => {
      var element = document.createElement("script");
      element.type = type, element.src = script;
      (b || document.body).appendChild(element);
    }, loadMultiple = (b, q, t) => {
      for (let i=0; i < q.length; i++) {
        loadJS(b, q[i], t);
      }
    }, fireEvent = () => {
      const event = new Event('afterQ');
      window.dispatchEvent(event);
    }, loadQ = () => {
      const b = document.body;
      setTimeout(function(){
        loadMultiple(b, scriptQLevel1), loadMultiple(b, moduleQLevel1, "module");
        setTimeout(function(){
          loadMultiple(b, moduleQLevel2, "module");
          setTimeout(function(){
            loadMultiple(b, moduleQLevel3, "module"), fireEvent();
          }, 200);
        }, 100);
      }, 0);
    }
    if (window.addEventListener) window.addEventListener("load", loadQ, false);
    else if (window.attachEvent) window.attachEvent("onload", loadQ);
    else window.onload = loadQ;

  </script>
